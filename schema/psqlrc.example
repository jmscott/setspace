/*
 *  Synopsis:
 *	Reasonable PostgreSQL psqlrc file that inclues all schemas in search path
 *  Note:
 *	Eventually will be autogenerated based schemas defined in schema.mk.
 */
set search_path to 
	drblob,
	expat2,
	jsonorg,
	libxml2,
	nistrand,
	pdfbox2,
	pgtexts,
	prefixio,
	setspace,
	udigset
;
