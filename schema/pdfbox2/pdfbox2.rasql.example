{
	"synopsis":		"PDFBox (version 2) Schema for SetSpace",
	"http-listen":		":8080",
	"rest-path-prefix":	"/pdfbox2",

	"sql-query-set": {
		"pdfq": {
			"source-path":	"pdfq.sql"
		}
	},
	"http-query-arg-set": {
		"q": {
			"sql-alias": "keywords"
		},
		"limit": {
			"default": "10",
			"matches": "^(?:10|100|1000)$",
			"sql-alias": "limit"
		},
		"offset": {
			"default": "0",
			"sql-alias": "offset"
		},
		"tsv": {
			"default": "english",
			"sql-alias": "ts_conf"
		}
	},
	"warn-slow-sql-query-duration": 5
}
