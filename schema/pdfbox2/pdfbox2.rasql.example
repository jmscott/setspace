{
	"synopsis":		"PDFBox (version 2) Schema for SetSpace",
	"http-listen":		":8080",
	"rest-path-prefix":	"/pdfbox2",

	"sql-query-set": {
		"pdfq-keyword": {
			"source-path":	"pdfq-keyword.sql"
		},
		"pdfq-phrase": {
			"source-path":	"pdfq-phrase.sql"
		}
	},
	"http-query-arg-set": {
		"k": {
			"sql-alias": "keyword"
		},
		"p": {
			"sql-alias": "phrase"
		},
		"tsc": {
			"default": "english",
			"sql-alias": "ts_conf"
		},
		"lim": {
			"default": "10",
			"matches": "^(?:10|100|1000)$",
			"sql-alias": "limit"
		},
		"ofs": {
			"default": "0",
			"sql-alias": "offset"
		}
	},
	"warn-slow-sql-query-duration": 5
}
