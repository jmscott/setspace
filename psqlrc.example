/*
 *  Synopsis:
 *	Example PostgreSQL psqlrc file that inclues all schemas in search path
 *  Note:
 *	Eventually search path will will be autogenerated based upon schemas
 *	defined in schema.mk.
 */
set search_path to 
	expat2,
	fffile5,
	jsonorg,
	libxml2,
	mycore,
	mydash,
	pdfbox,
	setcore,
	setfault,
	public
;
-- Show how long each query takes to execute
\timing

-- Use best available output format
\x auto
\set HISTFILE .psql_history- :DBNAME
\set HISTCONTROL ignoredups
