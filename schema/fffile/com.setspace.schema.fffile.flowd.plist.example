<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-/Apple/DTD PLIST 1.0/EN" "http:/www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.setspace.schema.fffile.flowd</string>

  <key>KeepAlive</key>
  <true/>

  <key>RunAtLoad</key>
  <true/>

  <key>ProcessType</key>
  <string>Background</string>

  <key>AbandonProcessGroup</key>
  <false/>

  <key>EnvironmentVariables</key>
  <dict>
  	<key>PATH</key>
	<string>/usr/local/setspace/schema/fffile/sbin:/usr/local/setspace/sbin:/usr/local/setspace/bin:/usr/local/blobio/bin:/usr/local/pgsql/bin:/usr/local/bin:/opt/local/bin:/usr/bin:/bin</string>

	<key>SETSPACE_ROOT</key>
	<string>/usr/local/setspace</string>
  </dict>

  <key>ProgramArguments</key>
  <array>
	<string>/usr/local/setspace/sbin/launchd-flowd</string>
	<string>etc/fffile.flow</string>
  </array>

  <key>Disabled</key>
  <false/>

  <key>UserName</key>
  <string>setspace</string>

  <key>GroupName</key>
  <string>staff</string>

  <key>WorkingDirectory</key>
  <string>/usr/local/setspace/schema/fffile</string>

  <key>StandardErrorPath</key>
  <string>/usr/local/setspace/schema/fffile/log/launchd-flowd.log</string>

  <key>StandardOutPath</key>
  <string>/usr/local/setspace/schema/fffile/log/launchd-flowd.log</string>

</dict>
</plist>
